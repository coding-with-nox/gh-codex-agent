# OpenAI
# The API key to authenticate with the OpenAI service
OPENAI_API_KEY=your-openai-api-key
# The OpenAI model to use (e.g., gpt-5-codex)
OPENAI_MODEL=gpt-5-codex

# GitHub
# The name of the user or organization owning the GitHub repository
GITHUB_OWNER=your-github-username-or-org
# The name of the GitHub repository to work on
GITHUB_REPO=your-repo-name
# The ID of the GitHub App used for authentication
GITHUB_APP_ID=your-github-app-id
# The installation ID of the GitHub App for the specific repository
GITHUB_INSTALLATION_ID=your-installation-id
# The private key of the GitHub App, encoded in Base64
GITHUB_PRIVATE_KEY_BASE64=your-private-key-base64
# The path to the file containing the private key of the GitHub App
GITHUB_PRIVATE_KEY_PATH=/path/to/private-key.pem

# Configuration
# The GitHub label used to filter issues to monitor
ISSUE_LABEL=agent
# The polling interval (in seconds) to check for new issues
POLL_INTERVAL_SECONDS=60
# The default base branch for the repository
DEFAULT_BASE_BRANCH=main
# The local working directory where the code will be cloned
WORKDIR=/work
# The HTTPS URL to clone the repository
REPO_CLONE_URL=https://github.com/your-github-username-or-org/your-repo-name.git